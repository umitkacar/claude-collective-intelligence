    {
      "type": "visualization",
      "id": "request-timeline",
      "attributes": {
        "title": "Request Timeline",
        "description": "Request count and response time trend",
        "visState": {
          "title": "Request Timeline",
          "type": "line",
          "params": {
            "grid": {
              "categoryLines": false,
              "valueAxis": "ValueAxis-1"
            },
            "categoryAxes": [
              {
                "id": "CategoryAxis-1",
                "type": "category",
                "scale": {
                  "type": "linear"
                },
                "labels": {
                  "show": true,
                  "truncate": 100
                },
                "title": {}
              }
            ],
            "valueAxes": [
              {
                "id": "ValueAxis-1",
                "name": "Left",
                "type": "value",
                "scale": {
                  "type": "linear"
                },
                "labels": {
                  "show": true
                },
                "title": {
                  "text": "Response Time (ms)"
                }
              },
              {
                "id": "ValueAxis-2",
                "name": "Right",
                "type": "value",
                "scale": {
                  "type": "linear"
                },
                "labels": {
                  "show": true
                },
                "title": {
                  "text": "Request Count"
                }
              }
            ],
            "seriesParams": [
              {
                "show": true,
                "type": "line",
                "interpolate": "linear",
                "valueAxis": "ValueAxis-1",
                "drawLinesBetweenPoints": true,
                "lineWidth": 2
              },
              {
                "show": true,
                "type": "line",
                "interpolate": "linear",
                "valueAxis": "ValueAxis-2",
                "drawLinesBetweenPoints": true,
                "lineWidth": 2
              }
            ]
          },
          "aggs": [
            {
              "id": "1",
              "enabled": true,
              "type": "avg",
              "params": {
                "field": "duration_ms",
                "customLabel": "Avg Response Time"
              },
              "schema": "metric"
            },
            {
              "id": "2",
              "enabled": true,
              "type": "count",
              "params": {},
              "schema": "metric"
            },
            {
              "id": "3",
              "enabled": true,
              "type": "date_histogram",
              "params": {
                "field": "@timestamp",
                "interval": "auto",
                "customInterval": "1m",
                "format": "strict_date_histogram",
                "min_doc_count": 0
              },
              "schema": "segment"
            }
          ]
        },
        "kibanaSavedObjectMeta": {
          "searchSourceJSON": {
            "query": {
              "match": {
                "http_method": {
                  "query": "*",
                  "type": "phrase"
                }
              }
            },
            "index": "logs-*",
            "filter": [
              {
                "exists": {
                  "field": "duration_ms"
                }
              }
            ]
          }
        }
      }
    },
